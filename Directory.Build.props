<Project>
    <PropertyGroup>
        <Product>Evolve Event Bus on Dapr</Product>
        <Authors>Alexander Bocharov</Authors>
        <Company>Alexander Bocharov</Company>
        <Copyright>Â© Alexander Bocharov. All rights reserved.</Copyright>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <PackageProjectUrl>https://github.com/evolvesw/evolve-eventbus-dapr</PackageProjectUrl>
        <RepositoryType>git</RepositoryType>
